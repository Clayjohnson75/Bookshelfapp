{
  "crons": [
    {
      "path": "/api/scan-job",
      "schedule": "* * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/search",
      "destination": "/api/search"
    },
    {
      "source": "/:username",
      "destination": "/api/profile/:username"
    }
  ]
}

